{
  "exclude": ["./coverage", "./dist", "__tests__", "jest.config.js"],

  "compilerOptions": {
    "types": ["jest", "node"],
    // "noEmit": true,  

    "target": "ES6",
    "module": "commonjs",
    "resolveJsonModule": true,

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*","docs/**/*"]
}
